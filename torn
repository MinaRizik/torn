game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(184.346008, 256.626831, -96.4693527)
wait(8.0)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2571.83057, 202.039444, 4619.75098)
wait(2.0)
